{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": [">0.25%","not op_mini all"]
        },
        "useBuiltIns": "usage",
        "corejs": "core-js@2"
    }],
    "@babel/preset-typescript",
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread",
    "@babel/plugin-transform-arrow-functions",
    "@babel/plugin-syntax-dynamic-import"
  ]
}
